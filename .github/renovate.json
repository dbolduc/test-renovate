{
  "extends": [
    "config:base",
    "docker:disable"
  ],
  "regexManagers": [
    {
      "fileMatch":  [".*\\.Dockerfile$"],
      "matchStrings": [
        "https://github\\.com/(?<depName>.*?)/archive/(?<currentValue>.*?)\\.tar\\.gz"
      ],
      "datasourceTemplate": "github-releases"
    }
  ],
  "ignoreDeps": [ "com_google_googleapis" ]
}
